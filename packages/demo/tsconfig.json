{
  "compilerOptions": {
    "outDir": "../../docs",
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "declaration": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build", "src/**/*.test.ts"]
}
